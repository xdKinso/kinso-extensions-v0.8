{
  "author": "Faizan Durrani @FaizanDurrani",
  "bin": {
    "paperback-cli": "./bin/run.js"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@oclif/core": "^4.5.2",
    "@oclif/plugin-help": "^6.2.32",
    "@oclif/plugin-plugins": "^5.4.46",
    "@paperback/runtime-polyfills": "*",
    "@paperback/types": "*",
    "@protobuf-ts/runtime-rpc": "^2.11.1",
    "buffer": "^6.0.3",
    "esbuild": "^0.25.9",
    "fs-extra": "^11.3.1",
    "listr2": "^8.3.3",
    "picocolors": "^1.1.1"
  },
  "description": "The Paperback extension development toolchain",
  "devDependencies": {
    "@oclif/test": "^4.1.13",
    "@protobuf-ts/plugin": "^2.11.1",
    "@protobuf-ts/protoc": "^2.11.1",
    "@types/chai": "^5.2.2",
    "@types/fs-extra": "^11.0.4",
    "@types/listr": "^0.14.9",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.17.2",
    "chai": "^5.3.1",
    "grpc-tools": "^1.13.0",
    "mocha": "^11.7.1",
    "oclif": "^4.22.12",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/FaizanDurrani/paperback-toolchain",
  "license": "MIT",
  "main": "lib/index.js",
  "name": "@paperback/toolchain",
  "oclif": {
    "bin": "paperback-cli",
    "dirname": "paperback-cli",
    "commands": "./lib/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
  },
  "repository": "FaizanDurrani/paperback-toolchain",
  "type": "module",
  "scripts": {
    "build": "shx rm -rf lib && tsc -b && cp -r src/homepage lib/ && cp -r src/shims lib/",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "version": "1.0.0-alpha.57",
  "bugs": "https://github.com/FaizanDurrani/paperback-toolchain/issues",
  "keywords": [
    "oclif"
  ],
  "types": "lib/index.d.ts",
  "gitHead": "feee6f52fe2e0d4f5aaeaa159c8bdd26b9fdfe82",
  "ts-node": {
    "esm": true
  }
}
